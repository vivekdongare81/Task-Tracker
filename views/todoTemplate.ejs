
  <%-include("header")-%>
  
  <div class="box" id="heading">
    <h1>
      <%= listTitle %>
    </h1>
    </div>
    <p class="para-Text">Save entered new Note by clicking "+" button and to Delete Note just click Checkbox :)</p>
  <div class="box">
    <% List_Array.forEach(function(element){ %>
      
      <form  action="/deleteNote" method="post">
    
      <div class="item">
         <input type="checkbox" name="checkbox" value="<%= element._id %>" onChange='submit();'></input>
   
        <p>
          <%= element.ListArray %>
        </p>
     </div>

     </form>
  <% }); %>
   
        <form class="item" action="/" method="post">

          <input type="text" name="newNote" placeholder="Add New Note" autocomplete="off" />
          <button type="submit" name="listType" value="<%= listTitle %>">+</button>

        </form>
  </div>

  <%-include("footer")-%>